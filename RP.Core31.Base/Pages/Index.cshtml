<environment include="Development,Staging">
    <div class="container-fluid">
        <div class="card text-center mt-5">
            <div class="card-header">
                <p>This section will only visible on your development or staging web server. In production environment it should automatically exclude the contents in the section. </p>
                <h1>Visual Studio Solution Name: <span class="text-success">@Html.DisplayFor(model => model.SolutionName)</span></h1>
            </div>
            <div class="card-body">
                <h3 class="card-title">Project Name : <span class="span-bold text-danger">@Html.DisplayFor(model => model.MyApplicationName)</span></h3>
                <p class="card-text"> ASPNETCORE_ENVIRONMENT : <span class="btn btn-danger">@Html.DisplayFor(model => model.ThisEnvironment)</span> </p>
                <p>Application Name : <span class="btn alert-primary">@Html.DisplayFor(model => model.MyApplicationName)</span> </p>
                <p>TargetFramework : <span class="btn alert-primary text-wrap">@Html.DisplayFor(model => model.TargetFramework)</span> </p>
                <p>Microsoft ASP.NET Core Version : <span class="btn alert-primary">@Html.DisplayFor(model => model.DotNetCoreVersion)</span> </p>
                <p>Current RunTime Version : <span class="btn alert-primary">@Html.DisplayFor(model => model.CurrentRunTimeVersion)</span> </p>
            </div>
            <div class="card-footer text-muted">
                <p><span class="btn alert-primary text-wrap">@Html.DisplayFor(model => model.version)</span> </p>
            </div>
        </div>
    </div>
</environment>